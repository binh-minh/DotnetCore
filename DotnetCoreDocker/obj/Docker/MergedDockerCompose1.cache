services:
  dotnetcoredocker:
    build:
      context: /Volumes/DATA/Research/DotnetCore/DotnetCoreDocker
      dockerfile: DotnetCoreDocker/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: DotnetCoreDocker
      target: base
    container_name: DotnetCoreDocker
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: dotnetcoredocker:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/netcoreapp3.1/DotnetCoreDocker.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - /Volumes/DATA/Research/DotnetCore/DotnetCoreDocker/DotnetCoreDocker:/app:rw
    - /Users/nguyenbinhminh/vsdbg/vs2017u5:/remote_debugger:rw
    - /Users/nguyenbinhminh/.aspnet/https:/root/.aspnet/https:ro
    - /Users/nguyenbinhminh/.microsoft/usersecrets:/root/.microsoft/usersecrets:ro
    - /Users/nguyenbinhminh/.nuget/packages:/root/.nuget/packages:ro
    - /Volumes/DATA/Research/DotnetCore/DotnetCoreDocker:/src:rw
version: '3.4'